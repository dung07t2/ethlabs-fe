{
  "name": "nextjs-base",
  "version": "1.0.0",
  "private": true,
  "moduleResolution": "node",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint-fix": "next lint --cache --cache-location .next/cache/eslint/ --fix",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:write": "yarn prettier:base --write \"./src/**/*.{ts,js,tsx,jsx}\"",
    "postinstall": "husky install",
    "format": "yarn prettier:write && yarn lint-fix",
    "cleani": "rm -rf node_modules package-lock.json yarn-error.lock yarn.lock .next && yarn cache clean --force && yarn",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:spec": "cypress run --spec"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,tsx,ts,jsx}": [
      "yarn format",
      "git add ."
    ]
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.0",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.94",
    "@mui/material": "^5.10.0",
    "@mui/styles": "^5.9.3",
    "@mui/x-data-grid": "^5.15.2",
    "@mui/x-date-pickers": "^5.0.0-beta.5",
    "@reduxjs/toolkit": "^1.8.4",
    "@tanstack/react-query": "^4.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "firebase": "^9.9.2",
    "formik": "^2.2.9",
    "framer-motion": "^6.2.8",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^12.2.5",
    "notistack": "^2.0.2",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.34.1",
    "react-hot-toast": "^2.3.0",
    "react-redux": "^8.0.2",
    "react-share": "^4.4.0",
    "react-truncate": "^2.4.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.45.0",
    "simplebar-react": "^2.3.6",
    "social-media-icons-react": "^1.1.10",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@next/bundle-analyzer": "^12.2.5",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^16.11.4",
    "@types/react": "^18.0.17",
    "@types/react-redux": "^7.1.24",
    "@types/react-truncate": "^2.3.4",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "^28.1.3",
    "cypress": "^10.4.0",
    "eslint": "^8.22.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  }
}
